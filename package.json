{
  "name": "json-prompt-generator-v2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://sisigitadi.github.io/promptmatrix2/",
  "scripts": {
    "dev": "vite",
    "build": "node scripts/generate-docs-list.js && npm run lint && vite build",
    "preview": "vite preview",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"./src/**/*.{ts,tsx}\" --fix",
    "format": "prettier \"./src/**/*.{ts,tsx,js,jsx,css,md}\" --write",
    "test": "set NODE_OPTIONS=--experimental-vm-modules && jest",
    "e2e": "playwright test",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "validate:frameworks": "ts-node scripts/validate-frameworks.mjs",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@google/generative-ai": "^0.24.1",
    "@vitejs/plugin-react": "^5.0.4",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "bootstrap": "^5.3.8",
    "diff": "^8.0.2",
    "react": "^19.2.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.6",
    "react-toastify": "^11.0.5"
  },
  "overrides": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/diff": "^7.0.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.7.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "cypress": "^15.4.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.4",
    "playwright": "^1.56.0",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.1",
    "vite": "^7.1.10"
  }
}
